    <combo_box name="WLWaterPresetsCombo" tool_tip="WindLight preestablecido para el Agua."/>
    <button label="&lt;" name="WWprev"/>
    <button label="&gt;" name="WWnext"/>
    <check_box label="Entorno de la Región" control_name="UseEnvironmentFromRegion" name="use_estate_wl" tool_tip="Usar la configuración de entorno de la región/parcela cuando esté disponible."/>
  <check_box label="¿Siempre?" control_name="UseEnvironmentFromRegionAlways" name="Always" tool_tip="Usar siempre la configuración de la región/parcela, cuando exista, en forma automática."/>
    <icon image_name="icon_day_cycle.tga"/>
    <slider control_name="EnvTimeSlider" label=""name="EnvTimeSlider"/>
